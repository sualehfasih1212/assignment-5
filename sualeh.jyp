    "palindrome = \"\"\n",
    "for letter in word[-1::-1].lower():\n",
    "    palindrome = palindrome + letter\n",
    "if palindrome == word.lower():\n",
    "    print(f\"Your Entered words \\\"{word}\\\" are Palindrome\")\n",
    "else:\n",
    "    print(f\"Your Entered words {word} are not Palindrome\")"
