   "source": [
    "def factorial(num):\n",
    "    if num == 0:\n",
    "        return 1\n",
    "    if num > 0:\n",
    "        result = num * factorial(num - 1)\n",
    "        return result\n",
    "\n",
    "num = int(input(\"Enter a Non-Negative-Integer Number:\"))\n",
    "\n",
    "if num >= 0:\n",
    "    result = factorial(num)\n",
    "    print(\"The Factorial of input Number are: \",result)\n",
    "else:\n",
    "    print(\"You Input a Wrong Number\")\n"
   ]
